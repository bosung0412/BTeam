<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ict.project.dao.BoardDao">
    <select id="selectList" resultType="bvo">
        select * from openforum
    </select>
    <insert id="addBoard" parameterType="bvo">
        insert into openforum values(openforum_seq.nextVal,#{id},#{oname},#{ocontent},sysdate,#{ocategory})
    </insert>
</mapper>