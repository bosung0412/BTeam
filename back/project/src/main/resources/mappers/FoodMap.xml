<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace ="kr.co.ict.project.dao.FoodDao">
    <select id="selectList" resultType = "Fvo">
    select * from FOODS
    </select>
    <select id="foodDetail" resultType="Fvo">
        select * from FOODS where fno = #{fno}
    </select>

<insert id="addFood" parameterType="Fvo">
    INSERT INTO FOODS (fno,files,fname,fprovince,ffprovince,fcholesterol,fsalt,fpotasium,fcarbohydrate,fdietaryfiber,fprotein,tocal)
    VALUES (FOODS_SEQ.nextVal,#{files, jdbcType=BLOB},#{fname},#{fprovince},#{ffprovince},#{fcholesterol},#{fsalt},#{fpotasium},#{fcarbohydrate},#{fdietaryfiber},#{fprotein},#{tocal})

</insert>

</mapper>