{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-ProjectApplication<project>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "kr.co.ict.project.ProjectApplication",
            "projectName": "project",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-BackApplication<back>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "kr.co.ict.back.BackApplication",
            "projectName": "back",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "pwa-chrome",
            "name": "vuejs: chrome",
            "request": "launch",
            "url": "http://192.168.0.115:8081/",
            "breakOnLoad": true,
            "webRoot": "${workspaceFolder}/front",
            "pathMapping": {
                "/_karma_webpack_": "${workspaceFolder}/front"
            },
            "outFiles": ["${workspaceFolder}/front/target/dist/**/*.js"],
            "vueComponentPaths": [
                "${workspaceFolder}/front/src/**/*.vue"
            ],
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/*"
            }
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Node: Nodemon",
            "processId": "${command:PickProcess}",
            "restart": true,
            "protocol": "inspector",
        },
        {
            "name": "Vue:firefox",
            "type": "firefox",
            "request": "launch",
            "url": "http://192.168.0.115:8081",
            "webRoot": "${workspaceFolder}/front/",
            "pathMappings": [
              {
                "url": "http://192.168.0.115",
                "path": "${workspaceFolder}"
              }
            ]
        },
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8081/",
            "webRoot": "${workspaceFolder}/front"
        },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/index.html"
        },

    ]
}